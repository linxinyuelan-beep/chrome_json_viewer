{
  "manifest_version": 3,
  "name": "Tab Title Changer",
  "version": "1.2.0",
  "description": "A Chrome extension that allows you to change tab titles.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "windows",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ]
}